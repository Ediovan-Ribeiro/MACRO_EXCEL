import os

# Caminho para o diretório de logs
PASTA_LOGS = '..\\logs\\execucao.log'

# Garantir que a pasta de logs exista
log_dir = os.path.dirname(PASTA_LOGS)
if not os.path.exists(log_dir):
    os.makedirs(log_dir)

# Agora o código para registrar no log pode ser executado
with open(PASTA_LOGS, 'a') as log:
    log.write('Mensagem de log\n')
